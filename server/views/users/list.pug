extends layouts/main-layout.pug

block content
    .main.container
        .row
            .col-md-12.col-md-offset-12
                .jumbotron
                    h3 List of Users
                    table.table.table-striped
                        tr
                            th username
                            th email
                            th password
                        each user in userArr
                            tr
                                td= user.username
                                td= user.email
                                td= user.password


